{
  "name": "cawsandra-migrate",
  "version": "0.0.1",
  "description": "Fork of cassandra-migrate for AWS Keyspaces",
  "bin": {
    "cawsandra": "./lib/cawsandra.js"
  },
  "main": "lib/cawsandra.js",
  "types": "lib/cawsandra.d.ts",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhoudu/cawsandra-migrate.git"
  },
  "keywords": [
    "aws",
    "keyspaces",
    "cassandra",
    "migrations",
    "migration",
    "tool",
    "cql",
    "node"
  ],
  "engines": {
    "node": ">= 0.18.0"
  },
  "author": "Benjamin Houdu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhoudu/cawsandra-migrate/issues"
  },
  "homepage": "https://github.com/bhoudu/cawsandra-migrate#readme",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc -p tsconfig.json",
    "test": "cross-env NODE_ENV?=test TZ=UTC jest --colors --detectOpenHandles --forceExit --no-cache --runInBand",
    "lint": "eslint 'src/**/*.ts' --fix-dry-run",
    "ping": "node lib/cawsandra.js ping",
    "ping:ts": "ts-node src/cawsandra.ts ping",
    "cql:ts": "ts-node src/cawsandra.ts cql ./resources/cql/selectMigrationTable.cql",
    "create": "node lib/program.js create 'test' -t ./migrations/template.tpl -m ./migrations",
    "create:ts": "ts-node src/cawsandra.ts create test_me -m ./migrations",
    "create:old": "node app.js create 'test' -t ./migrations/template.tpl -m ./migrations",
    "up": "ts-node src/cawsandra.ts up -m ./migrations",
    "up:old": "node app.js up -m ./migrations -o ./migrations/options.js --create",
    "down:old": "node app.js down -m ./migrations -o ./migrations/options.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "aws-sigv4-auth-cassandra-plugin": "^1.0.4",
    "cassandra-driver": "^4.6.4",
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.4.2",
    "jest-html-reporter": "^3.7.0",
    "jest-sonarcloud-reporter": "^1.0.1",
    "prettier": "^2.8.4",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
